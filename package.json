{
  "name": "rollup-plugin-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "author": "wood3n",
  "description": "rollup-plugin boilerplate, support typescript",
  "keywords": [
    "boilerplate",
    "rollup",
    "rollup-plugin",
    "typescript",
    "typescript-node",
    "node",
    "node-esmodule"
  ],
  "homepage": "https://github.com/wood3n/rollup-plugin-boilerplate#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "rollup -c rollup.config.ts --configPlugin typescript -w",
    "build": "rollup -c rollup.config.ts --configPlugin typescript"
  },
  "peerDependencies": {
    "rollup": "^1.20.0||^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-plugin-node": "^11.1.0",
    "rollup": "^2.79.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  }
}
